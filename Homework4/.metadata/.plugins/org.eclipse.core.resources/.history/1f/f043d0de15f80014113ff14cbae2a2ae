
public class Z3 {
	public static void main(String[] args) {
		
		int[][] arr = new int[5][6];
		int num = 30;
		
		for(int row = 0; row < 5; row++){
			for(int col = 0; col < 6; col++){
				arr[row][col] = num;
				num--;
				
			}
			
		}
		for(int row = 0; row < 5; row++){
			for(int col = 0; col < 6; col++){
				System.out.println(arr[row][col]);
			}
	} 
}
}