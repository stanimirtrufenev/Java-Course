import java.util.Scanner;

public class Z3 {

	public static void main(String[] args) {
		int smallest=0;
        int large=0; 
        int num=0;
		
		Scanner input = new Scanner(System.in, "UTF-8");
		
		System.out.println("Please enter number for N: ");
        int n = input.nextInt();
        
int i;

        for(i=0;i<n;i++)
        {
          num=input.nextInt();}
          if(num>large)
          {
           large=num;
          }
          System.out.println("the largest is:"+large);
          

     if(i==0 && num>0)
         smallest=num;
     if(num<smallest)
        smallest=num;
     System.out.println("the largest is:"+smallest);

      }
	}
