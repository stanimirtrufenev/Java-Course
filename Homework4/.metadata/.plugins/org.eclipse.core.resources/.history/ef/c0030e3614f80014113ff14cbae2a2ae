
public class Z3 {
	public static void main(String[] args) {
		
		int num = 30;
		int[][] masiv = new int[5][6];
		for (int row = 0; row <= 4; row++) {
			for (int col = 0; col <= 5; col++) {
				masiv[row][col] = num;
				num--;
				
				System.out.print(masiv[row][col] + "\t");
			}
			
			System.out.println();
		}
//		
//		for (int row = 0; row <= 4; row++) {
//			for (int col = 0; col <= 5; col++) {
//				System.out.print(masiv[row][col] + "\t");
//			}
//			System.out.println();
//		}
	} 
}
