
import java.util.Scanner;

public class Zadachka8 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in, "UTF-8");

		System.out.println("Please enter number for end: ");
        int n = input.nextInt();
        
        input.close();
        
        int[] arr = new int[n];

        for (int i = 1; i <= n; i++){
           arr[i-1] = i;
           System.out.println(i);
        }
        for (int i = n; i > 0; i--){
            arr[i-1] = i;
            System.out.println(i);
         }

	}
}
