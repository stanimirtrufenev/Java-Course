
public class Z3 {
	public static void main(String[] args) {
		
		int num = 30;
		int[][] masiv = new int[4][5];
		for (int row = 1; row < 5; row++) {
			for (int col = 1; col < 6; col++) {
				masiv[row][col] = num;
				num--;
			}
		}
		
		for (int row = 1; row < 5; row++) {
			for (int col = 1; col < 6; col++) {
				masiv[row][col] = num;
				num--;
			}
		}
	} 
}
