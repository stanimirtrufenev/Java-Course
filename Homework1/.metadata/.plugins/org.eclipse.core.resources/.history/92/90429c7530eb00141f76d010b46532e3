
public class Z2 {

	public static void main(String[] args) {
		int a = 1;
		System.out.print("a= ");
		System.out.println(a);
		
		int b = 3;
		System.out.print("b= ");
		System.out.println(b);
		
		int c = b;
		System.out.print("a is: ");
		System.out.println(a);
		
		c = a;
		System.out.print("b is: ");
		System.out.println(b);

	}

}
