import java.util.Scanner;

public class D2Z4 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in, "UTF-8");
		
		System.out.println("Please enter number: ");
        int num = input.nextInt();
        
        input.close();
        
        String word;
        
        switch (num) {
        case 0: word = "zero";
        break;
        case 1: word = "one";
        break;
        case 2: word = "two";
        break;
        case 3: word = "three";
        break;
        case 4: word = "four";
        break;
        case 5: word = "five";
        break;
        case 6: word = "six";
        break;
        case 7: word = "seven";
        break;
        case 8: word = "eight";
        break;
        case 9: word = "nine";
        break;
        }
        
        System.out.println(word);

	}

}
