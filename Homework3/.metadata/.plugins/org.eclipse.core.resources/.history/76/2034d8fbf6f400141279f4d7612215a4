import java.util.Scanner;

public class Z1 {

	public static void main(String[] args) {
        Scanner input = new Scanner(System.in, "UTF-8");
		
		System.out.println("Please enter number for N: ");
        int n = input.nextInt();
        
        input.close();
        
        for(int i = 0; i < n; i++){
        	System.out.print(i + " ");
        }

	}

}
