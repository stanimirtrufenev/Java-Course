
public class Zadachka7 {

	public static void main(String[] args) {
		int a = 1;
		print(10, a);

	}
	
	public static void print(int number, int a) {
	    if (number > 0) {
	    	number--;
	    	a *= number;
	    	print(number, a);
	    	System.out.println(number);
	    } else {
		      return;
	    }
	}
}
