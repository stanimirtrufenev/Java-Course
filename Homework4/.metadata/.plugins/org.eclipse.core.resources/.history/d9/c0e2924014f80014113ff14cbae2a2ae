
public class Z3 {
	public static void main(String[] args) {
		
		int[][] arr = new int[5][6];
		int num = 30;
		
		for(int row = 0; row < arr.length; row++){
			for(int col = 0; col < 6; col++){
				arr[row][col] = num;
				num--;
				System.out.println(arr[row][col]);
			}
		}
		System.out.print();
	} 
}
