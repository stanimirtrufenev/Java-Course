
public class Zadachka6 {

	public static void main(String[] args) {
		for(int i = 10; i <= 999; i++){
			
			print(i);
		}

	}
	
	public static void print(int i){
		if(i >= 10 && i <= 99){
			int a = i / 10;
			int b = i % 10;
			
			if(a == b){
				System.out.println(i);
			}
		}
		
		if(i > 99 && i <= 999){
			int c = i / 100;
			int t = i % 100;
			int u = t % 10;
			
			if(c == u){
				System.out.println(i);
			}
		}
	}

}
