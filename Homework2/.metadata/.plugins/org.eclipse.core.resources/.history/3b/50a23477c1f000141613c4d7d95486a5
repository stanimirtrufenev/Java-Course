        }
        if(a.equals("9")){
     	   System.out.println("Valid Card.");
        }
        else{
     	   System.out.println("Invalid Card.");
        }
        if(a.equals("10")){
     	   System.out.println("Valid Card.");
        }
        else{
     	   System.out.println("Invalid Card.");
        }
        if(a.equals("J")){
     	   System.out.println("Valid Card.");
        }
        else{
     	   System.out.println("Invalid Card.");
        }
        if(a.equals("Q")){
     	   System.out.println("Valid Card.");
        }
        else{
     	   System.out.println("Invalid Card.");
        }
        if(a.equals("K")){
     	   System.out.println("Valid Card.");
        }
        else{
     	   System.out.println("Invalid Card.");
        }
        if(a.equals("A")){
     	   System.out.println("Valid Card.");
        }
